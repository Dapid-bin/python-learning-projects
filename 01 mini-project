print("Check kelulusan")
print("1.Dapid 2.Icak 3.Amelia")

Nilai = input("Nilai split , : ").split(",")
nilai_Dapid, nilai_Icak, nilai_Amelia = Nilai

nilai = {
	"Dapid": int(nilai_Dapid.strip()),
	"Icak": int(nilai_Icak.strip()),
	"Amelia": int(nilai_Amelia.strip())
}

status = {nama: ("Lulus" if score >= 80 else "Tidak lulus")
	for nama, score in nilai.items()}

print(status)